a.s<-sd(a$rate)
b<- FA7 %>% filter(discrim ==0, contrast == 0.8, setsize == 1)
b.m <- mean(b$rate)
b.s<-sd(b$rate)
c<- FA7 %>% filter(discrim ==0, contrast == 0.5, setsize == 2)
c.m <- mean(c$rate)
c.s<-sd(c$rate)
d<- FA7 %>% filter(discrim ==0, contrast == 0.8, setsize == 2)
d.m <- mean(d$rate)
d.s<-sd(d$rate)
# there's a shorter way to do this
